{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./cypress/*"
      ],
    },
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "cypress",
      "@bahmutov/cy-api",
      "node"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "cypress/**/*.ts"
  ]
}